{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"ignorePatterns": [
		"node_modules/",
		"logs/",
		"uploads/"
	],
	"rules": {
		"no-unused-vars": ["error", { "args": "after-used", "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }],
		"no-console": "off"
	}
}
